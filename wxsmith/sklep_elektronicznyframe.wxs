<?xml version="1.0" encoding="utf-8" ?>
<wxsmith>
	<object class="wxFrame" name="sklep_elektronicznyFrame">
		<title>Sklep Elektroniczny</title>
		<size>515,283</size>
		<fg>#FF0000</fg>
		<bg>wxSYS_COLOUR_BACKGROUND</bg>
		<id_arg>0</id_arg>
		<handler function="OnClose" entry="EVT_CLOSE" />
		<object class="wxButton" name="ID_BUTTON1" variable="Button1" member="yes">
			<label>PRODUKTY</label>
			<pos>32,32</pos>
			<bg>wxSYS_COLOUR_WINDOW</bg>
			<handler function="OnButton1Click" entry="EVT_BUTTON" />
		</object>
		<object class="wxButton" name="ID_BUTTON2" variable="Button2" member="yes">
			<label>PRACOWNICY</label>
			<pos>128,32</pos>
			<bg>wxSYS_COLOUR_WINDOW</bg>
			<handler function="OnButton2Click" entry="EVT_BUTTON" />
		</object>
		<object class="wxButton" name="ID_BUTTON3" variable="Button3" member="yes">
			<label>KLIENCI</label>
			<pos>240,32</pos>
			<bg>wxSYS_COLOUR_WINDOW</bg>
			<handler function="OnButton3Click" entry="EVT_BUTTON" />
		</object>
		<object class="wxButton" name="ID_BUTTON4" variable="Button4" member="yes">
			<label>ZAMOWIENIA</label>
			<pos>336,32</pos>
			<bg>wxSYS_COLOUR_WINDOW</bg>
			<handler function="OnButton4Click" entry="EVT_BUTTON" />
		</object>
		<object class="wxStaticText" name="ID_STATICTEXT1" variable="StaticText1" member="yes">
			<label>Witamy!</label>
			<pos>160,96</pos>
			<fg>#80FF00</fg>
			<font>
				<size>26</size>
				<style>normal</style>
				<weight>bold</weight>
				<underlined>0</underlined>
				<family>swiss</family>
				<face>Microsoft YaHei</face>
			</font>
		</object>
		<object class="wxStaticText" name="ID_STATICTEXT2" variable="StaticText2" member="yes">
			<label>Program ten umozliwa zlozenie zamoweinia na elekronike z naszego sklepu.</label>
			<pos>8,200</pos>
			<fg>#80FF00</fg>
		</object>
		<object class="wxMenuBar" variable="MenuBar1" member="no">
			<object class="wxMenu" variable="Menu1" member="no">
				<label>&amp;File</label>
				<object class="wxMenuItem" name="idMenuQuit" variable="MenuItem1" member="no">
					<label>Quit</label>
					<accel>Alt-F4</accel>
					<help>Quit the application</help>
					<handler function="OnQuit" entry="EVT_MENU" />
				</object>
			</object>
			<object class="wxMenu" variable="Menu2" member="no">
				<label>Help</label>
				<object class="wxMenuItem" name="idMenuAbout" variable="MenuItem2" member="no">
					<label>About</label>
					<accel>F1</accel>
					<help>Show info about this application</help>
					<handler function="OnAbout" entry="EVT_MENU" />
				</object>
			</object>
		</object>
		<object class="wxStatusBar" name="ID_STATUSBAR1" variable="StatusBar1" member="yes">
			<fields>1</fields>
			<widths>-1</widths>
			<styles>wxSB_NORMAL</styles>
		</object>
	</object>
</wxsmith>
